{
  "$id": "https://github.com/nasa-gcn/gcn-schema/blob/master/core-schema/gcn.notices.core.Identifiers.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "gcn.notices.core.Identifiers",
  "description": "Identifiers of trigger and archive",
  "properties": {
    "instrument_id": {
      "type": "string",
      "description": "Trigger number or id number"
    },
    "instrument_id_alternate": {
      "type": ["string", "null"],
      "description": "Alternate id number"
    },
    "data_archive_page": {
      "type": ["string", "null"],
      "description": "URL of archived data files"
    }
  },
  "required": ["instrument_id", "instrument_id_alternate", "data_archive_page"]
}