{
  "$id": "https://gcn.nasa.gov/schema/main/gcn/notices/swift/bat/guano/alert.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Swift/BAT-GUANO Alert",
  "description": "Candidate gamma-ray transient reported from the BAT-GUANO targeted search",
  "type": "object",
  "allOf": [
    {
      "$ref": "/schema/main/gcn/notices/core/Alert.schema.json"
    },
    {
      "$ref": "/schema/main/gcn/notices/core/Event.schema.json"
    },
    {
      "$ref": "/schema/main/gcn/notices/core/DateTime.schema.json"
    },
    {
      "$ref": "/schema/main/gcn/notices/core/Localization.schema.json"
    },
    {
      "$ref": "/schema/main/gcn/notices/core/Statistics.schema.json"
    },
    {
      "$ref": "/schema/main/gcn/notices/core/Reporter.schema.json"
    },
    {
      "$ref": "/schema/main/gcn/notices/core/AdditionalInfo.schema.json"
    }
  ],
  "properties": {
    "$schema": true,
    "follow_up_event": {
      "type": "string",
      "description": "Name or trigger time of the external trigger that launched the search."
    },
    "follow_up_type": {
      "type": "string",
      "description": "Type of external trigger that launched the search, eg GW, neutrino, etc."
    }
  },
  "unevaluatedProperties": false
}
