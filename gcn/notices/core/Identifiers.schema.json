{
  "$id": "https://gcn.nasa.gov/schema/gcn/notices/core/Identifiers.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Identifiers",
  "description": "Identifiers of trigger and archive",
  "properties": {
    "event_id": {
      "type": "string",
      "description": "Trigger ID specific to the instrument (ex: Fermi bn230313485, Swift 1159327)"
    },
    "event_id_alternate": {
      "type": "string",
      "description": "Alternate instrument specific ID number"
    },
    "data_archive_page": {
      "type": "string",
      "description": "URL of archived data files"
    },
    "event_name": {
      "type": "string",
      "description": "Name of the event (ex: GRB 211211A, GW170817, AT2017gfo, SGR 1806-20, etc)"
    },
    "event_alias": {
      "type": "array",
      "description": "0 or more alias event names"
    }
  }
}
